{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Backend module",
      "type": "debugpy",
      "request": "launch",
      "module": "app",
      "console": "integratedTerminal",
      "envFile": "${workspaceFolder}/.env",
      "python": "${workspaceFolder}/.venv/bin/python"
    },
    {
      "name": "Debug RAG tool",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/src/backend/tools/rag.py",
      "console": "integratedTerminal",
      "envFile": "${workspaceFolder}/.env",
      "python": "${workspaceFolder}/.venv/bin/python"
    }
  ]
}
